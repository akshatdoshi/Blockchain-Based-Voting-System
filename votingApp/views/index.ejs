<script type="text/javascript">
// Load google charts
google.charts.load('current', {'packages':['corechart']});
google.charts.setOnLoadCallback(drawChart);

// Draw the chart and set the chart values
function drawChart() {
  var data = google.visualization.arrayToDataTable([
  ['<%=data[0].candidtaeName%>', '<%=data[0].voteCount%>'],
  ['<%=data[1].candidtaeName%>', '<%=data[1].voteCount%>'],
  ['<%=data[2].candidtaeName%>', '<%=data[2].voteCount%>']
  
]);
  // Optional; add a title and set the width and height of the chart
  var options = {'title':'My Average Day', 'width':550, 'height':400};

  // Display the chart inside the <div> element with id="piechart"
  var chart = new google.visualization.PieChart(document.getElementById('piechart'));
  chart.draw(data, options);
}
</script>
	
<script>
window.onload = function() {

var chart = new CanvasJS.Chart("chartContainer", {
	animationEnabled: true,
	title: {
		text: "Desktop Search Engine Market Share - 2016"
	},
	data: [{
		type: "pie",
		startAngle: 240,
		yValueFormatString: "##0.00\"%\"",
		indexLabel: "{label} {y}",
		dataPoints: [
			{label: '<%=data[0].candidtaeName%>', value: '<%=data[0].voteCount%>', color:'#167f39'},
			{label: '<%=data[1].candidtaeName%>', value: '<%=data[1].voteCount%>', color:'#FF0012'},
			{label: '<%=data[2].candidtaeName%>', value: '<%=data[2].voteCount%>', color:'#AF312'},
		]
	}]
});
chart.render();

}
</script>



