<%- include header.ejs %>
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Tables</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            DataTables Advanced Tables
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>Voter ID</th>
                                        <th>Voter Address</th>
                                        <th>First Name</th>
                                        <th>Last Name</th>
                                        <th>Gender</th>
                                        <th>Voter Voted</th>
                                    </tr>
                                </thead>
                                <tbody>
								
                                    <% if(data.length){ 
										for(var i = 0;i < data.length;i++) { %>
									  <tr>
										<td><%=data[i].voterID%></td>
										<td><%=data[i].voterAddress%></td>
										<td><%=data[i].voterFastName%></td>
										<td><%=data[i].voterLastName%></td>
										<td><%=data[i].voterGender%></td>
										<td><%=data[i].voterVoted%></td>
									  </tr>
									  <% }
									  }else{ %>
									  <tr>
										<td colspan="2">No user</td>
									  </tr>
									<% } %>

                                </tbody>
                            </table>

                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
        </div>
        <!-- /#page-wrapper -->

  <%- include footer.ejs %>
